{
  "name": "jeffrey",
  "version": "0.0.1",
  "description": "jefrrey chen's static website of node",
  "main": "./app.js",
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.config.dev.js",
    "dll-dev": "rm -fr ./src/dll/dev/&&webpack --config ./build/webpack.config.dll.dev.js",
    "dll-prod": "rm -fr ./src/dll/prod/&&webpack --config ./build/webpack.config.dll.prod.js",
    "dev": "rm -fr ./dist/recommend/&&webpack --config ./build/webpack.config.dev.js",
    "prod": "rm -fr ./dist/recommend/&&webpack --config ./build/webpack.config.product.js",
    "stops": "taskkill /f /im node.exe",
    "debug": "set NODE_ENV=production&&node --inspect-brk ./build/webpack.config.dev.js",
    "debug-base": "set NODE_ENV=production&&node --inspect-brk ./build/webpack.config.base.js",
    "debug-prod": "set NODE_ENV=production&&node --inspect-brk ./build/webpack.config.product.js",
    "debug-dll": "set NODE_ENV=production&&node --inspect-brk ./build/webpack.config.dll.dev.js",
    "debug-webpack": "set NODE_ENV=production&&node --inspect-brk ./node_modules/webpack/bin/webpack.js --config ./build/webpack.config.dev.js"
  },
  "author": "jeffrey chen",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.18.3",
    "@babel/runtime-corejs3": "^7.18.3",
    "ant-design-vue": "^1.7.8",
    "antd": "^3.26.19",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.22.7",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "fastclick": "^1.0.6",
    "file-loader": "^6.2.0",
    "fs": "^0.0.2",
    "html-loader": "^3.1.0",
    "ip": "^1.1.3",
    "js-base64": "^2.4.5",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "path": "^0.12.7",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "px2rem-loader": "^0.1.9",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "7.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "sass-loader": "^13.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "url-loader": "^4.1.1",
    "vuex": "3.0",
    "webpack": "^5.72.1"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2"
  }
}
